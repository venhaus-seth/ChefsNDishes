@model List<Chef>
<div class="text-center">
    <h3>Check out our roster of Chefs</h3>

    <table class="table">

        <tr>
            <th>Name</th>
            <th>Age</th>
            <th># of Dishes</th>
        </tr>

        @foreach(Chef c in Model)
        {
        <tr>
            <td>@c.FirstName @c.LastName</td>
            <td>@(DateTime.Now.Year - @c.DOB.Year)</td>
            <td>@c.Dishes.Count()</td>
        </tr>
        }

    </table>
    
</div>

